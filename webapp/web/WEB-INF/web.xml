<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>Testing</servlet-name>
        <servlet-class>com.webapp.servlets.Testing</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Testing2</servlet-name>
        <servlet-class>com.webapp.servlets.Testing2</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Testing3</servlet-name>
        <servlet-class>com.webapp.servlets.Testing3</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UtilityServlet</servlet-name>
        <servlet-class>com.webapp.servlets.UtilityServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Testing</servlet-name>
        <url-pattern>/Testing</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Testing2</servlet-name>
        <url-pattern>/Testing2</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Testing3</servlet-name>
        <url-pattern>/Testing3</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UtilityServlet</servlet-name>
        <url-pattern>/UtilityServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
